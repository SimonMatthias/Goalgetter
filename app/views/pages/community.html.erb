<body class="community">
    <div class="community-navbar">
      <div>
        <%= link_to "/", class: "navbar-brand" do %>
          <%= cl_image_tag("logo.png") %>
        <% end %>
      </div>
      <div class="image-circle-nav">
        <%= cl_image_tag current_user.photo.key, class: 'community-navbar-image' %>
      </div>
      <div>
        <h4 style="color: #64A4F4">Filter:</h4>
        <h4>Time</h4>
        <%= link_to '5 Year goals', '/', class: 'btn community-navbar-button-blue' %>
        <%= link_to 'Yearly goals', '/', class: 'btn community-navbar-button-blue' %>
        <%= link_to 'Quarterly goals', '/', class: 'btn community-navbar-button-blue' %>
        <%= link_to 'Weekly tasks', '/', class: 'btn community-navbar-button-blue' %>
        <h4>Category</h4>
        <%= link_to 'Financial', '/', class: 'btn community-navbar-button-red' %>
        <%= link_to 'Career', '/', class: 'btn community-navbar-button-red' %>
        <%= link_to 'Health', '/', class: 'btn community-navbar-button-red' %>
        <%= link_to 'Personal', '/', class: 'btn community-navbar-button-red' %>

        <%= link_to "Dashboard", dashboard_path, class: 'btn community-navbar-button-white' %>
        <%= link_to "Settings", "/settings", class: 'btn community-navbar-button-white' %>
        <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: 'btn community-navbar-button-white' %>
      </div>
    </div>

    <div class="community-content">
      <h1>Community</h1>
      <h2>Find goals of fellow Goalgetters</h2>
    </div>
</body>
